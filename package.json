{
  "name": "ota-dashboard",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.12.0"
  },
  "description": "OTA+ dashboard",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "BABEL_ENV=development NODE_ENV=development ./node_modules/webpack-cli/bin/cli.js --config webpack.config.js",
    "build:prod": "BABEL_ENV=production NODE_ENV=production ./node_modules/webpack-cli/bin/cli.js --config webpack.config.js",
    "build:marko": "NODE_ENV=production ./node_modules/.bin/marko compile --server",
    "build:clean": "./node_modules/.bin/marko compile --clean && rm -rf ./dist/",
    "start": "NODE_ENV=production NODE_PATH=. NODE_CONFIG_DIR=./config node --v8-pool-size=1500 ./bin/www",
    "dev": "npm run build:dev && DEBUG=request-promise NODE_ENV=development NODE_PATH=. NODE_CONFIG_DIR=./config node --v8-pool-size=1500 ./bin/www"
  },
  "keywords": [
    "ota+",
    "dashboard",
    "devices"
  ],
  "author": "Milo Casagrande <milo@foundries.io>",
  "license": "Apache-2.0",
